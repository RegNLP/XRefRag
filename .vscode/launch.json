{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "xrefrag generate (smoke)",
            "type": "python",
            "request": "launch",
            "module": "xrefrag.cli",
            "args": [
                "generate",
                "-c",
                "configs/project.yaml",
                "--preset",
                "smoke",
                "--method",
                "dpel",
                "--max-pairs",
                "1",
                "--row-sample-n",
                "1"
            ],
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal"
        }
    ]
}
